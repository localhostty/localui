{
  "name": "@local/ui",
  "version": "0.0.1",
  "author": "localhostty",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localhostty/localui.git"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "esbuild-plugin-solid": "^0.6.0",
    "jsdom": "^27.3.0",
    "solid-js": "^1.9.10",
    "tsup": "^8.5.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.9.3",
    "@biomejs/biome": "2.3.10",
    "ultracite": "6.5.0",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "solid-js": "^1.9.10"
  },
  "exports": {
    "./button": {
      "solid": "./dist/button/index.jsx",
      "import": {
        "types": "./dist/button/index.d.ts",
        "default": "./dist/button/index.js"
      }
    },
    "./collapsible": {
      "import": {
        "types": "./dist/collapsible/index.d.ts",
        "default": "./dist/collapsible/index.js"
      }
    }
  },
  "browser": {},
  "bugs": {
    "url": "https://github.com/localhostty/localui/issues"
  },
  "contributors": [],
  "description": "UI Component lib for SolidJS",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/localhostty/localui#readme",
  "keywords": [
    "solid"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "pnpm dlx ultracite fix",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "pnpm dlx ultracite check",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/button/index.js",
  "module": "./dist/button/index.js",
  "types": "./dist/button/index.d.ts",
  "typesVersions": {
    "*": {
      "button": [
        "./dist/button/index.d.ts"
      ],
      "collapsible": [
        "./dist/collapsible/index.d.ts"
      ]
    }
  }
}
