{
  "name": "localui",
  "version": "0.0.1",
  "description": "UI Component lib for SolidJS",
  "license": "MIT",
  "author": "localhostty",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/localhostty/localui.git"
  },
  "homepage": "https://github.com/localhostty/localui#readme",
  "bugs": {
    "url": "https://github.com/localhostty/localui/issues"
  },
  "files": [
    "dist"
  ],
  "private": false,
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently bun:test:*",
    "test:client": "vitest",
    "test:ssr": "bun run test:client --mode ssr",
    "prepublishOnly": "bun build",
    "format": "bunx ultracite fix",
    "lint": "concurrently bun:lint:*",
    "lint:code": "bunx ultracite check",
    "lint:types": "tsc --noEmit",
    "update-deps": "bun up -Li"
  },
  "peerDependencies": {
    "solid-js": "^1.9.10,"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@types/node": "^25.0.3",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "esbuild-plugin-solid": "^0.6.0",
    "jsdom": "^27.3.0",
    "solid-js": "^1.9.10",
    "tsup": "^8.5.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.9.3",
    "ultracite": "6.5.0",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "solid"
  ]
}
